package practica0;

public class Ejercicio19 {

	public static void main(String[] args) {
		String s="agil";
		String p= "agila";
		System.out.println(abecedaria(p));
		
		

	}
	public static boolean abecedaria(String s) {
		for(int i = 0; i<s.length()-1; i++) {
			if(s.charAt(i)>s.charAt(i+1)) {
				return true;
			}
		}
		return false;
	}

}
