package practica0;

//Escribir un método static int sumatoria(int n) que devuelve la sumatoria de los números
//desde 1 hasta n.

public class Ejercicio10 {

	public static void main(String[] args) {
		sumatoria(23);

	}
	public static int sumatoria(int n) {
		for(int i=1; i<=n; i++) {
			System.out.println(i);
		}
		return n;
		
		
	}
	

}
