package practica0;

public class Ejercicio20 {

	public static void main(String[] args) {
		String s="pete";
		System.out.println(esCapicua(s));
		

	}
	public static boolean esCapicua(String s) {
		s.toLowerCase();
		for(int i=0; i<s.length()/2;i++) {
			if(s.charAt(i)!=s.charAt(s.length()-1-i)) {
				return false;
			}
		}
		return true;

}
	
}
