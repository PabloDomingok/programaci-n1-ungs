package practica0;

public class Ejercicio22 {

	public static void main(String[] args) {
		String s="casos";
		System.out.println(sinRepetidos(s));
		

	}
	public static String sinRepetidos(String s){
		String palabra="";
		for(int i=0;i<s.length()-1; i++) {
			
				if(palabra.indexOf(i)==s.charAt(i)) {
					palabra+=s.charAt(i);

				}
			}
		
		return palabra;
		
	}
}	